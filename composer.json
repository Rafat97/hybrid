{
    "name": "root10/hybrid",
    "type": "hybrid",
    "description": "A Hybrid Framework.",
    "license": "MIT",
    "keywords": [
        "framework",
        "Hybrid"
    ],
    "require": {
        "php": "^7.1",
        "fzaninotto/faker": "^1.8",
        "guzzlehttp/guzzle": "^6.4",
        "phpunit/phpunit": "^8.4",
        "psr/log": "^1.1",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/console": "^4.3",
        "symfony/debug": "^4.3",
        "symfony/http-foundation": "^4.3",
        "symfony/http-kernel": "^4.3",
        "symfony/monolog-bundle": "^3.4",
        "symfony/translation": "^4.3",
        "symfony/twig-bridge": "^4.3",
        "symfony/var-dumper": "^4.3",
        "twig/twig": "^2.12"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
	"autoload": {
        "psr-4": {
            "App\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
